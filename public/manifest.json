{
  "name": "Feature Flags",
  "description": "Chrome feature flags extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "tools.png",
    "48": "tools.png",
    "128": "tools.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "host_permissions": ["<all_urls>"]
}